import React from 'react';
import {useParams} from "react-router-dom";

import "./{{pascalCase name}}.scss";

const {{pascalCase name}} = props => {

   let { {{lowerCase dynamic}} }= useParams();

  return (
    <div className="{{lowerCase name}}">
      <p>{{pascalCase name}}</p>
      {
        {{lowerCase dynamic}} && <p>{{lowerCase dynamic}} : { {{lowerCase dynamic}} }</p>
      }
    </div>
  );
};

export default {{pascalCase name}};